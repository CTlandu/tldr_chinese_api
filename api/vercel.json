{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./Backend/"
}
